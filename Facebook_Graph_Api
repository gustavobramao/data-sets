#Retrive data per AdSets

CURL GET 
act_10155071198470936/insights?level=ad&day&fields=campaign_id,campaign_name,adset_id,adset_name,spend&time_range={"since":"2018-12-01","until":"2018-12-10"}

#Set up multi_cell per campaing using CURL
curl \
-F 'name=UK - MULTI - PROSP' \
-F 'description="iterating multi cell per CPA"' \
-F 'start_time=1544443260' \
-F 'end_time=1545044460' \
-F 'cooldown_start_time=1544443260' \
-F 'observation_end_time=1545217260' \
-F 'viewers=[100000846856799]' \
-F 'type=LIFT' \
-F 'cells=[{name:"PROSP CPA 18",description:"",treatment_percentage:18,control_percentage:15,campaigns:[23843065340640315]},{name:"PROSP CPA 13",description:"",treatment_percentage:18,control_percentage:15,campaigns:[23843065337730315]},{name:"PROSP CPA 8",description:"",treatment_percentage:18,control_percentage:15,campaigns:[23843062459180315]}]' \
-F 'objectives=[{name:"Purchase",is_primary:true,type:"SALES",adspixels:[{id:395108407337683,event_names:["fb_pixel_purchase"]}],applications:[{id:632909560200169,event_names:["fb_mobile_purchase"]}]},{name:"MAI objective",type:"MAI",applications:[{id:632909560200169}]},{name:"(Other) Quality Session",type:"NONSALES",adspixels:[{id:395108407337683,event_names:["fb_pixel_lead"]}]},{name:"(Other) View Content",type:"NONSALES",adspixels:[{id:395108407337683,event_names:["fb_pixel_view_content"]}],applications:[{id:632909560200169,event_names:["fb_mobile_content_view"]}]},{name:"(Other) Add to Cart",type:"NONSALES",adspixels:[{id:395108407337683,event_names:["fb_pixel_add_to_cart"]}],applications:[{id:632909560200169,event_names:["fb_mobile_add_to_cart"]}]},{name:"(Other) Initiate Checkout",type:"NONSALES",adspixels:[{id:395108407337683,event_names:["fb_pixel_initiate_checkout"]}],applications:[{id:632909560200169,event_names:["fb_mobile_initiated_checkout"]}]}]' \
